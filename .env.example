# Configuration Firebase - Copiez ce fichier vers .env et remplissez vos valeurs
VITE_FIREBASE_API_KEY=your_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id

# Clé de chiffrement pour les mots de passe (générez une clé unique en production)
VITE_ENCRYPTION_KEY=your-secret-encryption-key-change-in-production

# Configuration du logging sécurisé
# Activer les logs dans la console (développement)
VITE_ENABLE_CONSOLE_LOGS=true
# Activer les logs en production (désactivé par défaut pour la sécurité)
VITE_ENABLE_PRODUCTION_LOGS=false
# Niveau de logging: debug, info, warn, error, system
VITE_LOG_LEVEL=debug